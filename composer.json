{
    "name": "novuso/common-bundle",
    "description": "Novuso Common Symfony Bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "~6.2",
        "guzzlehttp/promises": "~1.2",
        "guzzlehttp/psr7": "~1.3",
        "psr/cache": "~1.0",
        "psr/http-message": "~1.0",
        "psr/log": "~1.0",
        "novuso/common": "~0.2",
        "novuso/system": "~0.2",
        "symfony/monolog-bundle": "~2.8",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/symfony": "~3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.6",
        "mikey179/vfsStream": "~1.6",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.5",
        "squizlabs/php_codesniffer": "~2.6"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\Common\\Adapter\\": "src",
            "Novuso\\Test\\Common\\Adapter\\": "test"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
