{
    "name": "novuso/common-bundle",
    "description": "Novuso Common Symfony Bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/promises": "~1.0",
        "guzzlehttp/psr7": "~1.0",
        "psr/container": "1.*",
        "psr/http-message": "1.*",
        "psr/log": "1.*",
        "novuso/common": "dev-feature/refactor",
        "novuso/system": "0.5.*",
        "symfony/monolog-bundle": "~2.8|~3.0",
        "symfony/swiftmailer-bundle": "~2.0",
        "symfony/symfony": "~3.3",
        "willdurand/negotiation": "~2.0"
    },
    "require-dev": {
        "doctrine/orm": "2.5.*",
        "fzaninotto/faker": "1.6.*",
        "mikey179/vfsStream": "1.6.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "6.2.*",
        "squizlabs/php_codesniffer": "3.0.*"
    },
    "autoload": {
        "psr-4": {
            "Novuso\\Common\\Adapter\\": "src"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Novuso\\Test\\Common\\Adapter\\": "test"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
